        "def isPrime(n):\r\n",
        " \r\n",
        "    # Corner case\r\n",
        "    if (n <= 1):\r\n",
        "        return False\r\n",
        " \r\n",
        "    # Check from 2 to n-1\r\n",
        "    for i in range(2, n):\r\n",
        "        if (n % i == 0):\r\n",
        "            return False\r\n",
        " \r\n",
        "    return True\r\n",
        "\r\n",
        "isPrime(7)\r\n",
        " \r\n",
        " \r\n"
      ],
